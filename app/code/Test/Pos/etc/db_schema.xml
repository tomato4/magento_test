<schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Setup/Declaration/Schema/etc/schema.xsd">
        <table name="pos_entity">
            <column xsi:type="int" name="pos_id" unsigned="true" nullable="false" comment="ID"/>
            <column xsi:type="varchar" name="name" nullable="false" length="255" comment="Name of POS"/>
            <column xsi:type="varchar" name="address" nullable="false" length="255" comment="Address of POS"/>
            <column xsi:type="boolean" name="is_available" nullable="false" comment="Availability of POS"/>
            <constraint xsi:type="primary" referenceId="PRIMARY">
                <column name="pos_id"/>
            </constraint>
        </table>
</schema>
